{
  "name": "lendsqr-be-demo-wallet",
  "version": "1.0.0",
  "description": "An MVP (Minimum viable product)  wallet service for Demo Credit",
  "author": "Uchenna Emeruche",
  "scripts": {
    "start": "node dist/server/index.js",
    "start:dev": "nodemon src/server/index.ts",
    "build": "tsc",
    "test": "jest",
    "db:create": "ts-node ./src/scripts/createdb.ts",
    "migrate:make": "knex --knexfile src/db/knexfile.ts migrate:make",
    "migrate:up": "knex --knexfile src/db/knexfile.ts migrate:up",
    "migrate:latest": "knex --knexfile src/db/knexfile.ts migrate:latest",
    "migrate:rollback:last": "knex --knexfile src/db/knexfile.ts migrate:down",
    "migrate:rollback:latest": "knex --knexfile src/db/knexfile.ts migrate:rollback",
    "migrate:rollback:all": "knex --knexfile src/db/knexfile.ts migrate:rollback --all",
    "seed:make": "knex --knexfile src/db/knexfile.ts seed:make",
    "seed:run": "knex --knexfile src/db/knexfile.ts seed:run",
    "prepare": "husky install"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.6",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mock-knex": "^0.4.4",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "knex-mock-client": "^1.11.0",
    "mock-knex": "^0.4.12",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/compression": "^1.7.2",
    "axios": "^1.2.3",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.1",
    "mysql2": "^3.0.1",
    "node-cache": "^5.1.2",
    "superstruct": "^1.0.3",
    "winston": "^3.8.2",
    "zod": "^3.20.2"
  },
  "engines": {
    "node": "16.x"
  }
}
